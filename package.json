{
  "name": "solux-react-ts",
  "version": "0.0.1",
  "type": "module",
  "private": true,
  "scripts": {
    "build:server": "tsc -p tsconfig.server.json",
    "dev:server": "node --env-file=.env server.js",
    "dev:web": "vite",
    "dev:stable": "npm run build:server && concurrently -n SERVER,WEB \"npm:dev:server\" \"npm:dev:web\"",
    "dev:ts": "tsc -p tsconfig.server.json -w",
    "dev:all": "concurrently -k -n TS,SERVER,WEB \"npm:dev:ts\" \"npm:dev:server\" \"npm:dev:web\"",
    "build:web": "vite build",
    "build": "npm run build:server && npm run build:web",
    "start": "node --env-file=.env server.js",
    "check:env": "node --env-file=.env scripts/check-solux-env.mjs"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@prisma/client": "^6.14.0",
    "axios": "^1.6.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "prisma": "^6.13.0",
    "puppeteer": "^24.15.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^7.7.1",
    "undici": "^7.13.0",
    "zod": "^4.1.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/node": "^20.19.11",
    "@types/puppeteer": "^5.4.7",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.7.0",
    "autoprefixer": "^10.4.17",
    "concurrently": "^8.2.2",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "vite": "^7.0.6"
  }
}
